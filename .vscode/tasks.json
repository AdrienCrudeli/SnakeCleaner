{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build SnakeProject",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Snake project/SnakeProject.csproj",
        "/property:GenerateFullPaths=true"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "run SnakeProject",
      "type": "process",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Snake project/SnakeProject.csproj"
      ]
    }
  ]
}
